{
  "name": "bonsai-assessment",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "dev": "NODE_ENV=development rollup -c -w",
    "format": "prettier --write \"src/**/*.{js,jsx,css}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,css}\"",
    "format:watch": "onchange --await-write-finish 0 \"src/**/*.{js,jsx,json,css}\" -- sh -c 'clear; npx prettier --write {{changed}} && echo FORMAT OK'",
    "lint:check": "eslint -c .eslintrc.json src/**/*.{js,jsx}",
    "lint:watch": "onchange \"src/**/*.{js,jsx}\" -- sh -c 'clear; npx eslint -c .eslintrc.json {{changed}} && echo LINTER OK'",
    "test": "NODE_ENV=test mocha --exit --require @babel/register init.test.js \"src/**/*.test.{js,jsx}\"",
    "test:watch": "npx onchange \"src/**/*.{js,jsx}\" -- sh -c 'clear; NODE_ENV=test mocha --exit --require @babel/register init.test.js `node -p \"process.argv[1].replace(/^(.*?)(?:\\.test)?(\\.jsx?)$/,\\\"\\\\$1.test\\\\$2\\\")\" {{changed}}`' && echo TEST OK",
    "test:coverage": "NODE_ENV=test nyc --all npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laszlokardinal/bonsai-assessment.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/laszlokardinal/bonsai-assessment/issues"
  },
  "homepage": "https://github.com/laszlokardinal/bonsai-assessment#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-flexbox-grid": "^2.1.2",
    "react-redux-root": "^0.1.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.0.5",
    "reduxen": "^0.1.1",
    "reduxen-react-dom": "^0.1.1",
    "rollup-plugin-json": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@redux-saga/testing-utils": "^1.0.5",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "chai-diff": "^1.0.1",
    "chai-enzyme": "^1.0.0-beta.1",
    "dotenv": "^8.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-mocha": "^6.0.0",
    "eslint-plugin-react": "^7.14.3",
    "jsdom": "^15.1.1",
    "mocha": "^6.2.0",
    "mock-css-modules": "^1.0.0",
    "mock-require": "^3.0.3",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "pretty-format": "^24.8.0",
    "redux-logger": "3.0.0",
    "rollup": "^1.19.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
